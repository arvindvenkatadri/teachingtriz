table-source type=\"application/json\">\n{\"columns\":[{\"label\":[\"syllable\"],\"name\":[1],\"type\":[\"chr\"],\"align\":[\"left\"]},{\"label\":[\"syl_scores\"],\"name\":[2],\"type\":[\"dbl\"],\"align\":[\"right\"]}],\"data\":[{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.42857143\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.71428571\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"1.00000000\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"1.00000000\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.85714286\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.71428571\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.85714286\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"1.00000000\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.85714286\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.71428571\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.42857143\"},{\"1\":\"syllable_5\",\"2\":\"0.57142857\"},{\"1\":\"syllable_2\",\"2\":\"0.71428571\"},{\"1\":\"syllable_5\",\"2\":\"0.57100000\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.57100000\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.71400000\"},{\"1\":\"syllable_2\",\"2\":\"0.57142857\"},{\"1\":\"syllable_5\",\"2\":\"0.28500000\"},{\"1\":\"syllable_2\",\"2\":\"1.00000000\"},{\"1\":\"syllable_5\",\"2\":\"0.51400000\"},{\"1\":\"sy